    function toolbar_action_handler(arg) {
        var value;
        switch (arg.item.id) {
            case 'bold':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.toggleBold();
                }
                break;
            case 'italic':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.toggleItalic();
                }
                break;
            case 'underline':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.toggleUnderline('Single');
                }
                break;
            case 'strikethrough':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.toggleStrikethrough('SingleStrike');
                }
                break;
            case 'subscript':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.toggleSubscript();
                }
                break;
            case 'superscript':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.toggleSuperscript();
                }
                break;
            case 'align-justify':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.toggleTextAlignment('Justify');
                }
                break;
            case 'align-center':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.toggleTextAlignment('Center');
                }
                break;
            case 'align-right':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.toggleTextAlignment('Right');
                }
                break;
            case 'align-left':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.toggleTextAlignment('Left');
                }
                break;
            case 'new':
                documenteditor.openBlank();
                break;
            case 'open':
                //var fileUpload = document.getElementById('uploadfileButton');
                //fileUpload.value = '';
                //fileUpload.click();
                break;
            case 'left-indent':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.increaseIndent();
                }
                break;
            case 'right-indent':
                if (!documenteditor.isReadOnly && documenteditor.editor) {
                    documenteditor.editor.decreaseIndent();
                }
                break;
            case 'print':
                documenteditor.print();
                break;
            case 'undo':
                if (documenteditor.editorHistory) {
                    documenteditor.editorHistory.undo();
                }
                break;
            case 'redo':
                if (documenteditor.editorHistory) {
                    documenteditor.editorHistory.redo();
                }
                break;
            case 'insert_image':
                var pictureUpload = document.getElementById('insertImageButton');
                pictureUpload.value = '';
                pictureUpload.click();
                break;
            case 'insert_hyperlink':
                documenteditor.showDialog('Hyperlink');
                break;
            case 'insert_table':
                documenteditor.showDialog('Table');
                break;
            case 'find':
                documenteditor.showOptionsPane();
                break;
            case 'save':
                onExportClick();
                break;
            case 'download':
                if (docinfo) {
                    var a = document.createElement("a");
                    document.body.appendChild(a);
                    a.style = "display: none";
                    a.href = docinfo.Path;
                    a.download = docinfo.Name+'.docx';
                    a.click();
                    document.body.removeChild(a);
                }

                break;
            case 'font':
                documenteditor.showDialog('Font');
                break;
            case 'paragraph':
                documenteditor.showDialog('Paragraph');
                break;


        }
    }

    this.onInsertImage = function (args) {
        if (
            navigator.userAgent.match('Chrome') ||
            navigator.userAgent.match('Firefox') ||
            navigator.userAgent.match('Edge') ||
            navigator.userAgent.match('MSIE') ||
            navigator.userAgent.match('.NET')
        ) {
            if (args.target.files[0]) {
                var path = args.target.files[0];
                var reader = new FileReader();
                reader.onload = function (frEvent) {
                    var base64String = frEvent.target.result;
                    var image = document.createElement('img');
                    image.addEventListener('load', function () {
                        documenteditor.editor.insertImage(
                            base64String,
                            image.width,
                            image.height
                        );
                    });
                    image.src = base64String;
                };
                reader.readAsDataURL(path);
            }
        } else {
            var image = document.createElement('img');
            image.addEventListener('load', function () {
                documenteditor.editor.insertImage(args.target.value);
            });
            image.src = args.target.value;
        }
    };

    this.onFileChange = function (args) {
        if (args.target.files[0]) {
            var path = args.target.files[0];
            if (path.name.substr(path.name.lastIndexOf('.')) === '.sfdt') {
                var fileReader = new FileReader();
                fileReader.onload = function (e) {
                    var contents = e.target.result;
                    documenteditor.open(contents);
                };
                fileReader.readAsText(path);
                documenteditor.documentName = path.name.substr(
                    0,
                    path.name.lastIndexOf('.')
                );
            } else {
                loadFile(path);
            }
        }
        event.preventDefault();
    };

    //document.getElementById('uploadfileButton').addEventListener('change', onFileChange);

    document.getElementById('insertImageButton').addEventListener('change', onInsertImage);

    documenteditor.requestNavigate = function (args) {
        if (args.linkType !== 'Bookmark') {
            var link = args.navigationLink;
            if (args.localReference.length > 0) {
                link += '#' + args.localReference;
            }
            window.open(link);
            args.isHandled = true;
        }
    };

