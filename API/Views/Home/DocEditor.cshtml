    documenteditor.documentChange = function () {
        contentChanges = false;
        var save=document.getElementById('save');
        toolBar.enableItems(save.parentElement, false);
        var documentName;
        if (documenteditor.documentName === '') {
            documentName = 'Untitled';
        } else {
            documentName = documenteditor.documentName;
        }
        document.getElementById('titlebar').innerHTML = documentName;
    };

    documenteditor.contentChange = function () {
        setTimeout(function () {
            contentChanges = true;
            var save=document.getElementById('save');
            toolBar.enableItems(save.parentElement, true);
            onContentChange();
        }, 20);
    };

