    function onExportClick() {
        contentChanges=false;
        var save = document.getElementById('save');
        save.innerText = 'Saving...'
        var savingmessage = document.getElementById('status-saving');
        savingmessage.style.display = "block";

        toolBar.enableItems(save.parentElement, false);
        documenteditor.saveAsBlob('Docx').then(function(exportedDocument) {
            // The blob can be processed further
            var fileName=documenteditor.documentName+".docx";
            var formData = new FormData();
            formData.append('fileName', fileName);
            formData.append('data', exportedDocument);
            var httpRequest = new XMLHttpRequest();

            httpRequest.open('POST', '/api/CUS/DOC/File/DocumentEditor/Save/' + @ViewBag.FileID+'?IDPartner=' + @ViewBag.Partner, true);
            httpRequest.setRequestHeader('Authorization', 'Bearer ' + '@ViewBag.Token');

            httpRequest.onreadystatechange = function () {
                if (httpRequest.readyState === 4) {
                    save.innerText = 'Save';

                    if (httpRequest.status === 200 || httpRequest.status === 204) {
                        savingmessage.style.display = "none";
                    } else {
                        //alert('Fail to save the document');
                    }
                    getDocInfo();
                }
            }
            httpRequest.send(formData);
        });
    }

